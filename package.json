{
  "name": "typeahead-examples",
  "version": "0.6.0",
  "private": true,
  "homepage": "http://typeahead.tomocchino.com",
  "scripts": {
    "test": "jest --watch",
    "dev": "next dev",
    "lint": "next lint",
