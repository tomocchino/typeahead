    "postdist": "rm -rf package"
  },
  "dependencies": {
    "emojibase": "^6.1.0",
    "next": "^12.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "babel-jest": "^28.1.3",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28.1.3",
    "typescript": "^4.4.2"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
}
