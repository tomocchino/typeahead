{
  "name": "typeahead-examples",
  "version": "0.6.0",
  "private": true,
  "homepage": "http://typeahead.tomocchino.com",
  "scripts": {
    "test": "jest --watch",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "build": "next build",
    "start": "next start",
    "predist": "mkdir -p build package && cp -R {release/*,src} package",
    "dist": "cd package && tsc && cp -R src/css dist && npm pack --pack-destination ../build",
    "postdist": "rm -rf package"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.0.11",
    "emojibase": "^15.3.0",
    "next": "15.1.0",
    "node-fetch": "^3.3.2",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.4.0",
    "eslint-config-next": "15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2"
    }
  }
}
