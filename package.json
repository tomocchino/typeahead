    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/node-fetch": "^2.6.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
