{
  "name": "examples",
  "version": "0.6.0",
  "private": true,
  "homepage": "http://typeahead.tomocchino.com",
  "scripts": {
    "test": "jest --watch",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "build": "next build",
    "start": "next start",
    "predist": "mkdir -p build package && cp -R {release/*,src} package",
